{
  "name": "didueat",
  "version": "1.0.0",
  "description": "Meal and water intake tracking app with sharing features",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "node server/index.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm install && npm run build"
  },
  "keywords": [
    "meal-tracker",
    "health",
    "food-diary",
    "water-tracker"
  ],
  "author": "Osaym Omar",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
